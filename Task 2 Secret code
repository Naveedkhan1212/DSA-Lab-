#include<iostream>
using namespace std;

// program to find factoral of a number mean  secret code

void factorial(int *n)
{
	int fact=1;
	for(int i=1;i<=*n;i++)
	{
		fact*=i;
		
	}
	cout<<"the factoral of a number"<<*n<<" is "<<fact<<endl;
}
int main()
{
	int num;
	cout<<"enter a number:";
	cin>>num;
	factorial(&num);
	return 0;
}
